{
     // Hover to view descriptions of existing attributes.
  
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}/react-app/build"
        },
        {
            "name": "Debug React Tests",
            "type": "node",
            "request": "launch",
            //"port":9229,
            "runtimeExecutable": "${workspaceRoot}/react-app/node_modules/.bin/react-scripts",
            "runtimeArgs": [
              "--inspect-brk",
              "test"
            ],
            "args": [
              "--runInBand",
              "--no-cache",
              "--env=jsdom"
            ],
            "cwd": "${workspaceRoot}/react-app",
            //"protocol": "inspector",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach",
            "port": 9229,
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "meteor://ðŸ’»app/*": "${workspaceFolder}/*",
                "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
                "webpack://?:*/*": "${workspaceFolder}/*",
                "webpack:///./src/*": "${workspaceFolder}/*",
            }
        }
    ]
}